<?xml version='1.0' encoding='utf-8'?>
<widget id="nl.famaerts.carpark" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>CarPark</name>
    <description>
        Just another unbrick game.
    </description>
    <author email="jaerts66@gmail.com" href="http://famaerts.nl">
        John Aerts
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
